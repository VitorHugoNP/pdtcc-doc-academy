@model pdtcc_doc_academy.Models.Protocolo // Substitua pelo namespace correto do seu modelo

@{
    ViewData["Title"] = "Protocolo de Atestado de Matrícula";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h2>Detalhes do Protocolo</h2>
    <p><strong>Tipo de Documento:</strong> @Model.tipo_Doc</p>
    <p><strong>ID do Aluno:</strong> @Model.fk_aluno</p>
    <p><strong>ID do Funcionário:</strong> @Model.fk_func</p>
    <p><strong>Data de Emissão:</strong> @DateTime.Now.ToString("dd/MM/yyyy")</p> <!-- Exemplo de data -->
</div>

<a href="@Url.Action("Index", "Escolas")">Voltar</a> <!-- Link para voltar à lista ou página anterior -->